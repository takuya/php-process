{
  "name": "takuya/process",
  "description": "php proc_open wrapper class for shell command process management.",
  "homepage": "https://github.com/takuya/php-process",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "version": "1.3.3",
  "keywords": [
    "process",
    "proc_open",
    "command",
    "pipe"
  ],
  "authors": [
    {
      "name": "takuya",
      "email": "takuya.1st+nospam@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  
  "autoload": {
    "psr-4": {
      "SystemUtil\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
